import { IApp } from '@/types/nuxt'
import { LogType } from '@/services/LogService'
import <%= name %>Entity from '@/entities/<%= name %>'
import { errors } from '@/utils/error'

export default class Delete<%= name %>UseCase implements BaseUseCase {
  app: IApp
  constructor(app: IApp) {
    this.app = app
  }

  async execute(entity: <%= name %>Entity) {
    const { state, <%= name.toLowerCase() %>, <%= appName.toLowerCase() %>Gateway, logService } = this.app
    try {
      await <%= appName.toLowerCase() %>Gateway.<%= name %>.Delete<%= name %>(entity.id)
      <%= name.toLowerCase() %>.delete(entity.props)
      return true
    } catch (error) {
      await logService.handle({ type: LogType.Error, error })
      state.errors = errors(error)
      return ''
    }
  }
}
