import { IApp } from '@/types/nuxt'
import { LogType } from '@/services/LogService'
import { errors } from '@/utils/error'

export default class Fetch<%= name %>sUseCase implements BaseUseCase {
  app: IApp
  constructor(app: IApp) {
    this.app = app
  }

  async execute() {
    const { state, <%= name.toLowerCase() %>, <%= appName.toLowerCase() %>Gateway, logService } = this.app
    try {
      const props = await <%= appName.toLowerCase() %>Gateway.<%= name %>.Get<%= names %>()
      <%= name.toLowerCase() %>.save([])
      <%= name.toLowerCase() %>.save(props)
      return true
    } catch (error) {
      await logService.handle({ type: LogType.Error, error })
      state.errors = errors(error)
      return false
    }
  }
}
