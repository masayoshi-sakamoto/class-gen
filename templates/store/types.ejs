import { I<%= name %>Props } from '@/entities/<%= name %>'
import { IOptionsProps } from '@/entities/Options'

export const Types = {
  store: 'store',
  remove: 'remove',
  clear: 'clear'
}

export const path = '<%= name.toLowerCase() %>/'

export class Store implements FluxStandardAction<{ items: I<%= name %>Props[]; options?: IOptionsProps }> {
  type = path + Types.store
  constructor(public payload: { items: I<%= name %>Props[]; options?: IOptionsProps }) {}
}

export class Remove implements FluxStandardAction<I<%= name %>Props> {
  type = path + Types.remove
  constructor(public payload: I<%= name %>Props) {}
}

export class Clear implements FluxStandardAction<void> {
  type = path + Types.clear
  constructor() {}
}